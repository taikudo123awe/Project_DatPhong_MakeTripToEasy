  <script src="https://cdn.jsdelivr.net/npm/flatpickr"></script>
  <script>
        // Sample data structure matching your EJS variables
        const sampleCustomer = null; // Set to null to show login buttons, or {} to hide them
        const sampleRooms = [
            {
                roomId: 1,
                roomName: "Presidential Suite Hướng Biển",
                description: "Căn suite tổng thống với tầm nhìn panorama ra biển, nội thất cao cấp và dịch vụ butler 24/7",
                capacity: 2,
                price: 8500000,
                image: "presidential-suite.jpg"
            },
            {
                roomId: 2,
                roomName: "Royal Family Villa",
                description: "Biệt thự hoàng gia rộng rãi với hồ bơi riêng, vườn nhiệt đới và không gian sang trọng",
                capacity: 6,
                price: 15000000,
                image: "royal-villa.jpg"
            },
            {
                roomId: 3,
                roomName: "Diamond Deluxe Room",
                description: "Phòng deluxe cao cấp với thiết kế hiện đại, ban công riêng và view thành phố tuyệt đẹp",
                capacity: 2,
                price: 4200000,
                image: "diamond-deluxe.jpg"
            },
            {
                roomId: 4,
                roomName: "Golden Penthouse",
                description: "Penthouse đỉnh cao với sân thượng riêng, jacuzzi và tầm nhìn 360 độ ra toàn thành phố",
                capacity: 4,
                price: 25000000,
                image: "golden-penthouse.jpg"
            }
        ];

        // Initialize the page
        document.addEventListener('DOMContentLoaded', function() {
            renderAuthButtons();
            renderRooms();
            initializeDatePickers();
            initializeSearch();
        });

        function renderAuthButtons() {
            const authSection = document.getElementById('authSection');
            
            if (!sampleCustomer) {
                authSection.innerHTML = `
                    <a href="/login" class="btn btn-success">
                        <i class="fas fa-sign-in-alt"></i> Đăng nhập VIP
                    </a>
                    <a href="/provider/register" class="btn btn-warning">
                        <i class="fas fa-crown"></i> Trở thành đối tác
                    </a>
                `;
            } else {
                authSection.innerHTML = `
                    <div style="color: #fbbf24; font-weight: 700; font-size: 1.1rem;">
                        <i class="fas fa-user-crown"></i> Chào mừng quý khách, ${sampleCustomer.name || 'VIP Member'}!
                    </div>
                `;
            }
        }

        function renderRooms() {
            const roomsContainer = document.getElementById('roomsContainer');
            
            if (sampleRooms.length === 0) {
                roomsContainer.innerHTML = `
                    <div style="text-align: center; padding: 4rem; color: #d1d5db;">
                        <i class="fas fa-gem" style="font-size: 4rem; margin-bottom: 2rem; opacity: 0.5; color: #f59e0b;"></i>
                        <h3 style="color: #fbbf24;">Không tìm thấy phòng phù hợp</h3>
                        <p>Vui lòng điều chỉnh tiêu chí tìm kiếm để khám phá những trải nghiệm tuyệt vời</p>
                    </div>
                `;
                return;
            }

            roomsContainer.innerHTML = sampleRooms.map(room => `
                <div class="room-card">
                    <img src="https://images.unsplash.com/photo-1582719478250-c89cae4dc85b?w=400&h=220&fit=crop&auto=format" 
                         class="room-image" 
                         alt="${room.roomName}"
                         onerror="this.src='data:image/svg+xml;base64,'; this.alt='Luxury Room Image';">
                    
                    <div class="room-content">
                        <h3 class="room-title">${room.roomName}</h3>
                        <p class="room-description">
                            ${room.description.length > 80 ? room.description.slice(0, 80) + '...' : room.description}
                        </p>
                        
                        <div class="room-details">
                            <div class="room-capacity">
                                <i class="fas fa-users"></i>
                                <span>${room.capacity} khách VIP</span>
                            </div>
                            <div class="room-price">
                                ${room.price.toLocaleString()} VND
                                <span class="room-price-unit">/ đêm</span>
                            </div>
                        </div>
                    </div>
                    
                    <div class="room-footer">
                        <a href="/rooms/${room.roomId}" class="detail-btn">
                            <i class="fas fa-eye"></i> Khám phá ngay
                        </a>
                    </div>
                </div>
            `).join('');
        }

        function initializeDatePickers() {
            const checkinInput = document.querySelector('input[name="checkin"]');
            const checkoutInput = document.querySelector('input[name="checkout"]');
            
            if (checkinInput && checkoutInput) {
                const today = new Date().toISOString().split('T')[0];
                checkinInput.min = today;
                checkoutInput.min = today;
                
                checkinInput.addEventListener('change', function() {
                    checkoutInput.min = this.value;
                    if (checkoutInput.value && checkoutInput.value <= this.value) {
                        checkoutInput.value = '';
                    }
                });
            }
        }

        function initializeSearch() {
            const searchForm = document.getElementById('searchForm');
            const searchBtn = searchForm.querySelector('.search-btn');
            const spinner = searchBtn.querySelector('.loading-spinner');
            const btnText = searchBtn.querySelector('span');
            
            searchForm.addEventListener('submit', function(e) {
                e.preventDefault();
                
                spinner.style.display = 'block';
                btnText.textContent = 'Đang tìm kiếm...';
                searchBtn.disabled = true;
                
                setTimeout(() => {
                    spinner.style.display = 'none';
                    btnText.textContent = 'Tìm kiếm';
                    searchBtn.disabled = false;
                    
                    console.log('Luxury search completed');
                }, 2000);
            });
        }

        // Element SDK configuration
        const defaultConfig = {
            primary_color: "#f59e0b",
            secondary_color: "#d97706", 
            accent_color: "#fbbf24",
            success_color: "#059669",
            hero_title: "Chào mừng đến với Make Trip To Easy!",
            hero_subtitle: "Trải nghiệm sang trọng - Dịch vụ đẳng cấp - Kỷ niệm vĩnh cửu",
            section_title: "Bộ sưu tập phòng đẳng cấp"
        };

        async function render(config) {
            // Update text content
            const heroTitle = document.getElementById('heroTitle');
            const heroSubtitle = document.getElementById('heroSubtitle');
            const sectionTitle = document.getElementById('sectionTitle');
            
            if (heroTitle) {
                heroTitle.textContent = config.hero_title || defaultConfig.hero_title;
            }
            if (heroSubtitle) {
                heroSubtitle.textContent = config.hero_subtitle || defaultConfig.hero_subtitle;
            }
            if (sectionTitle) {
                sectionTitle.innerHTML = `<i class="fas fa-gem"></i> ${config.section_title || defaultConfig.section_title}`;
            }
            
            // Update colors
            const root = document.documentElement;
            root.style.setProperty('--primary-color', config.primary_color || defaultConfig.primary_color);
            root.style.setProperty('--secondary-color', config.secondary_color || defaultConfig.secondary_color);
            root.style.setProperty('--accent-color', config.accent_color || defaultConfig.accent_color);
            root.style.setProperty('--success-color', config.success_color || defaultConfig.success_color);
            
            // Update gradient backgrounds
            const primaryColor = config.primary_color || defaultConfig.primary_color;
            const secondaryColor = config.secondary_color || defaultConfig.secondary_color;
            const accentColor = config.accent_color || defaultConfig.accent_color;
